###############################################################################
# itsme configuration file (.its)
#
# Saturn 1B
#
# Font: https://en.wikipedia.org/wiki/Saturn_IB
###############################################################################

###############################################################################
[vehicle] # Vehicle parameters
# Payload mass [kg]
Mu = 8500
#
# Number of stages
NStag = 2
#
# Specific impulse [s]
Isp = 272, 420
#
# Structural inefficiency (defined by Cornelisse as efficiency)
efes = 0.095, 0.093
#
# Thrust [kN]
T = 7100, 890
#
# softness of the transions of propulsive curve
softness = 0.5
#
# Aerodynamic coefficients
#
CL0 = 0.0
#
# (B1 Miele 1998)
CL1 = 0.8
#
#
CD0 = 0.5
#
# (A2 Miele 1998)
CD2 = 0.5
#
# Reference area in [km2]
s_ref = 34.2e-06
#
###############################################################################
[initial] # Initial state constants
#
# Initial altitude[km]
h = 0.0
#
# Initial velocity [km/s]
V = 1.0e-6
#
# Initial flight path angle [째]
gamma = 90.0
#
###############################################################################
[final] # Final state constants
#
# Final orbital altitude [km]
h = 400.0
#
# Final flight path angle [째]
gamma = 0.0
#
###############################################################################
[trajectory] # Trajectory parameters
#
# Maximal angle of attack for attitude maneuver [째]
AoAmax = 2.0
#
# Initial time for attitude maneuver [s]
tAoA1 = 2.0
#
# Duration of attitude maneuver [째]
tAoA = 2.0
#
###############################################################################
[enviroment] # Enviroment constants
#
# Gravitational parameter [km3/s2]
GM = 398600.4415
#
# Earth radius [km]
R = 6371.0
#
# Earth angular velocity of rotation [rad/s]
we = 0.0
#
###############################################################################
[solver] # Solver parameters
#
guess = 1.0, 1.5, 1.0
#
limit = 1.0, 1.5, 1.0
#
tol = 1e-7
#
fracVel = 0.7
#
Ndiv = 100