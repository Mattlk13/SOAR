###############################################################################
# itsme configuration file (.its)
#
# author: Carlos Souza
###############################################################################

###############################################################################
[vehicle] # Vehicle parameters
# Vehicle parameters homogenity
homogeneous = 0
#
# Payload mass [kg]
Mu = 7150.0
#
# Number of stages
NStag = 3
#
# First stages specific impulse [s]
Isp1 = 250.0
#
# Final stage specific impulse [s]
Isp2 = 318.0
#
# Structural inefficiency (defined by Cornelisse as efficiency)
efes = 0.0965, 0.0721, 0.0103
#
# Thrust [kN]
T = 3252.0, 779.0, 298.0
#
# softness of the transions of propulsive curve
softness = 0.05
#
# Aerodynamic coefficients
# (B0 Miele 1998)
CL0 = 0.0
#
# (B1 Miele 1998)
CL1 = 0.8
#
# (A0 Miele 1998)
CD0 = 0.5
#
# (A2 Miele 1998)
CD2 = 0.0
#
# Reference area in [km2]
s_ref = 7.853981633974482e-07
#
###############################################################################
[initial] # Initial state constants
#
# Initial altitude[km]
h = 0.0
#
# Initial velocity [km/2]
V = 1.0e-6
#
# Initial flight path angle [째]
gamma = 90.0
#
###############################################################################
[final] # Final state constants
#
# Final orbital altitude [km]
h = 200.0
#
# Final flight path angle [째]
gamma = 0.0
#
###############################################################################
[trajectory] # Trajectory parameters
#
# Maximal angle of attack for attitude maneuver [째]
AoAmax = 2.0
#
# Initial time for attitude maneuver [s]
tAoA1 = 3.7
#
# Duration of attitude maneuver [째]
tAoA = 5.0
#
###############################################################################
[enviroment] # Enviroment constants
#
# Gravitational parameter [km3/s2]
GM = 398600.4415
#
# Earth radius [km]
R = 6371.0
#
# Earth amgular velocity of ratation [rad/2]
we = 7.27220521664304e-05
#
###############################################################################
[solver] # Solver parameters
#
guess = 4.0, 1.6, 1.5
#
limit = 1.0, 0.4, 0.4
#
tol = 1e-8